<?xml version="1.0" encoding="utf-8"?>
<!-- ColorStateList for invalid input text and warn icon -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:state_enabled="true"
        android:color="@color/colorInvalidInputEnabled" />

    <item
        android:state_enabled="false"
        android:color="@color/colorInvalidInputDisabled" />

</selector>
